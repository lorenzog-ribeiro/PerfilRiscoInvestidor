#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/l.ribeiro/Documents/Lorenzo/repos/PerfilRiscoInvestidor/api-backend/node_modules/.pnpm/vite@6.2.6_@types+node@22.14.1_tsx@4.19.3/node_modules/vite/bin/node_modules:/mnt/c/Users/l.ribeiro/Documents/Lorenzo/repos/PerfilRiscoInvestidor/api-backend/node_modules/.pnpm/vite@6.2.6_@types+node@22.14.1_tsx@4.19.3/node_modules/vite/node_modules:/mnt/c/Users/l.ribeiro/Documents/Lorenzo/repos/PerfilRiscoInvestidor/api-backend/node_modules/.pnpm/vite@6.2.6_@types+node@22.14.1_tsx@4.19.3/node_modules:/mnt/c/Users/l.ribeiro/Documents/Lorenzo/repos/PerfilRiscoInvestidor/api-backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/l.ribeiro/Documents/Lorenzo/repos/PerfilRiscoInvestidor/api-backend/node_modules/.pnpm/vite@6.2.6_@types+node@22.14.1_tsx@4.19.3/node_modules/vite/bin/node_modules:/mnt/c/Users/l.ribeiro/Documents/Lorenzo/repos/PerfilRiscoInvestidor/api-backend/node_modules/.pnpm/vite@6.2.6_@types+node@22.14.1_tsx@4.19.3/node_modules/vite/node_modules:/mnt/c/Users/l.ribeiro/Documents/Lorenzo/repos/PerfilRiscoInvestidor/api-backend/node_modules/.pnpm/vite@6.2.6_@types+node@22.14.1_tsx@4.19.3/node_modules:/mnt/c/Users/l.ribeiro/Documents/Lorenzo/repos/PerfilRiscoInvestidor/api-backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
